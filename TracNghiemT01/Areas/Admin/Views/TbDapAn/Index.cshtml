                @Html.DisplayNameFor(model => model.NoiDung)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DapAnDung)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ThuTu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ThuTuHienThi)
